{
  "manifest_version": 3,
  "name": "HelpSeller",
  "version": "1.0.0",
  "description": "Coach de vendas em tempo real com IA",
  "permissions": [
    "activeTab",
    "tabCapture",
    "offscreen",
    "storage",
    "tabs",
    "audioCapture"
  ],
  "host_permissions": [
    "https://meet.google.com/*",
    "https://*.zoom.us/*",
    "https://app.zoom.us/*",
    "http://localhost:3000/*",
    "https://*.vercel.app/*"
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/index.tsx-loader-DXSih5O4.js"
      ],
      "matches": [
        "https://meet.google.com/*",
        "https://*.zoom.us/*"
      ],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "src/index.css",
        "logo.svg"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "https://*.zoom.us/*",
        "https://meet.google.com/*"
      ],
      "resources": [
        "assets/theme-UeVto8tc.js",
        "assets/auth-DmuFIzIA.js",
        "assets/index.tsx-C1EZ5Dv2.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
